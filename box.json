{
    "alias": "oc-cli.phar",
    "main": "bin/oc",
    "output": "oc-cli.phar",
    "chmod": "0755",
    "directories": [
        "src"
    ],
    "files": [
        "LICENSE"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": ["Tests", "tests"],
            "in": "vendor"
        }
    ],
    "git-version": "package_version",
    "datetime": "package_datetime",
    "compactors": [
        "KevinGH\\Box\\Compactor\\Php"
    ],
    "compression": "GZ",
    "algorithm": "SHA1"
}