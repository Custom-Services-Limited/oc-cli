{
    "name": "custom-services-limited/oc-cli",
    "description": "Command line interface for OpenCart - Created by Custom Services Limited",
    "type": "project",
    "keywords": ["opencart", "cli", "command-line", "ecommerce"],
    "homepage": "https://github.com/Custom-Services-Limited/oc-cli",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Custom Services Limited",
            "email": "info@opencartgreece.gr",
            "homepage": "https://support.opencartgreece.gr/"
        }
    ],
    "require": {
        "php": ">=7.0",
        "symfony/console": "^3.4|^4.0|^5.0|^6.0",
        "symfony/yaml": "^3.4|^4.0|^5.0|^6.0",
        "symfony/filesystem": "^3.4|^4.0|^5.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5|^7.0|^8.0|^9.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "OpenCart\\CLI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenCart\\CLI\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/oc"
    ],
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.0"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    }
}